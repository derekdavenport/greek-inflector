{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib": ["es2017"],
    "sourceMap": true,
		"outDir": "out",
		"strict": true,
		"noUnusedLocals": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedParameters": true,
		"removeComments": true,
  },
  "exclude": [
    "node_modules"
  ]
}
